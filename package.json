{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "start:dev": "next dev --watch",
    "setadmin": "curl http://localhost:3000/api/register -d '{\"name\": \"Test User\", \"email\":\"test@test.com\", \"password\": \"password123\"}'",
